{
    "Comment": "A state machine that runs nqueens.",
    "StartAt": "Get Input",
    "States": {
        "Get Input": {
            "Type": "Task",
            "Resource": "${MakeInputFunctionArn}",
            "Next": "Run N-Queens"
        },
        "Run N-Queens": {
            "Type": "Task",
            "Resource": "${RunNQueensFunctionArn}",
            "Parameters": {
                "Records": [
                  {
                    "eventVersion": "2.0",
                    "eventSource": "aws:s3",
                    "awsRegion": "us-east-1",
                    "eventTime": "1970-01-01T00:00:00.000Z",
                    "eventName": "ObjectCreated:Put",
                    "userIdentity": {
                      "principalId": "EXAMPLE"
                    },
                    "requestParameters": {
                      "sourceIPAddress": "127.0.0.1"
                    },
                    "responseElements": {
                      "x-amz-request-id": "EXAMPLE123456789",
                      "x-amz-id-2": "EXAMPLE123/5678abcdefghijklambdaisawesome/mnopqrstuvwxyzABCDEFGH"
                    },
                    "s3": {
                      "s3SchemaVersion": "1.0",
                      "configurationId": "testConfigRule",
                      "bucket": {
                        "name": "lees-stepper-input",
                        "ownerIdentity": {
                          "principalId": "EXAMPLE"
                        },
                        "arn": "arn:aws:s3:::lees-stepper-input"
                      },
                      "object": {
                        "key": "foo",
                        "size": 1,
                        "eTag": "1",
                        "sequencer": "1"
                      }
                    }
                  }
                ]
              },
            "End": true
        }
    }
}
